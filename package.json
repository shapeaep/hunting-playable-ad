{
  "name": "hunting-playable-ad",
  "version": "1.0.0",
  "description": "Playable ad for hunting mobile game",
  "scripts": {
    "dev": "webpack serve --mode development --open",
    "build": "webpack --mode production",
    "build:analyze": "webpack --mode production --analyze",
    "serve": "node server.js",
    "start": "node server.js",
    "playable": "playable-scripts build",
    "playable:google": "playable-scripts build --network google",
    "playable:facebook": "playable-scripts build --network facebook",
    "playable:unity": "playable-scripts build --network unity",
    "playable:ironsource": "playable-scripts build --network ironsource",
    "playable:applovin": "playable-scripts build --network applovin",
    "playable:mintegral": "playable-scripts build --network mintegral",
    "playable:vungle": "playable-scripts build --network vungle",
    "playable:tiktok": "playable-scripts build --network tiktok",
    "playable:all": "playable-scripts build --all"
  },
  "devDependencies": {
    "copy-webpack-plugin": "^13.0.1",
    "css-loader": "^6.8.1",
    "gltf-pipeline": "^4.3.0",
    "html-webpack-plugin": "^5.5.3",
    "mini-css-extract-plugin": "^2.7.6",
    "style-loader": "^3.3.3",
    "terser-webpack-plugin": "^5.3.9",
    "webpack": "^5.89.0",
    "webpack-bundle-analyzer": "^4.10.1",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  },
  "dependencies": {
    "@smoud/playable-scripts": "^1.0.40",
    "@smoud/playable-sdk": "^1.0.23",
    "draco3dgltf": "^1.5.7",
    "three": "^0.160.0"
  }
}
